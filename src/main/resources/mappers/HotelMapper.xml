<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<!-- Mapper Registry in the MyBatis Configuration file is in effect -->
<mapper namespace="org.zerock.myapp.mybatis.mapper.HotelMapper">

    <select id="selectByCityId" resultType="Hotel">
        select city, name, address, zip from hotel where city = #{id}
    </select>

</mapper>
